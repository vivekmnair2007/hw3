<h1><%= @place["name"] %></h1>


  <% if @entries.present? %> <!-- Check if @entries exists and has data -->
    <% for entry in @entries %> <!-- Iterate over each entry correctly -->
        <h3><%= entry["title"] %></h3>
        <p><%= entry["occurred_on"] %></p>
        <p><%= entry["description"] %></p>
    <% end %>
  <% else %>
    <p>No entries found.</p>
  <% end %>


<p>
  <a href="/entries/new?id=<%= @place["id"] %>">Add a new post</a>
</p>
<p>
  <a href="/">Go back to places list</a>
</p>
